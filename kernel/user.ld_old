ENTRY(user_shell_main)

SECTIONS {
  . = 0x80200000;

  .text : {
    KEEP(*(.text.user_shell_main))    /* Force at beginning */
    *(.text*)                /* Then include the rest */
  }

  .rodata : {
    *(.rodata*)
  }

  .data : {
    *(.data*)
  }

  .bss : {
    *(.bss*)
    *(COMMON)
  }
}
